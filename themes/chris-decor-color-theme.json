{
  "name": "Chris Decor",
	"type": "dark",
	"colors": {
		"editor.background": "#202a30",
		"editor.foreground": "#cccccc",
		"activityBarBadge.background": "#007acc",
		"sideBar.background": "#28343b",
		"activityBar.background": "#202a30",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
    {
			"name": "Comment - Triple",
			"scope": [
				"comment.line.triple-slash",
        "comment.line.triple-slash punctuation.definition.comment",
			],
			"settings": {
				"fontStyle": "italic bold",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#08dada"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword.control",
				"keyword.other.special-method",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#b9358d", // magenta
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"constant.other.color",
				"meta.tag",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
			],
			"settings": {
				"foreground": "#47c285",
			}
		},
    {
      "name": "Functions - Call",
      "scope": [
        "support.function",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions - Call - Special",
      "scope": [
        "support.function.core",
        "support.function.kernel",
      ],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178" // orange
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		},
    {
      "name": "Constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
		{
			"name": "Symbols, Inherited Class, Markup Heading",
