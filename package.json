{
  "version": "0.1.1",
  "name": "chris-decor",
  "displayName": "Chris' Decor",
  "publisher": "trinhngocthuyen",
  "description": "Yet another theme for VSCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/trinhngocthuyen/vstheme.git"
  },
  "license": "MIT",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Chris' Decor",
        "uiTheme": "vs-dark",
        "path": "./themes/chris-decor-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "xcconfig",
        "extensions": [".xcconfig"]
      }
    ],
    "grammars": [
      {
        "language": "xcconfig",
        "scopeName": "source.xcconfig",
        "path": "./syntaxes/xcconfig.tmLanguage.json"
      }
    ]
  }
}